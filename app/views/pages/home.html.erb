<body  class='index-page unloaded'>

  <img class="loadBackground" src="/assets/bck.png" alt="" />

  <div class='Preloader text-center'>
    <span class='Preload-end'><img class="Preload-endImg" src="/assets/logo/classic.svg" alt="" /></span>
    <span class='Preload-start'><img class="Preload-startImg" src="/assets/logo/preload.svg" alt="" /></span>
  </div>

  <div id="parallax" class="Parallax">

    <img data-speed="0.1" class="parallaxBackground" src="/assets/bck.png" alt="" />


    <div id="intro">

      <img src="/assets/mainPage/intro/1.png" class="intro-pic intro-1stImg" alt="" />
      <img data-speed="1.5" src="/assets/mainPage/intro/2.png" class="intro-pic" alt="" />
      <img data-speed="2.5" src="/assets/mainPage/intro/3.png" class="intro-pic" alt="" />

      <div class="fullScreen topCaption" data-speed="4">
        <h1>Web development<br>at Dot where creativity<br>meets technology</h1>
      </div>

      <img data-speed="5.5" class='test-subject' src="/assets/mainPage/intro/4.png" alt="" />
    </div>


    <div id="bcode">

      <div class="fullScreen" data-speed="1">
        <div class="table">
          <div class="cell">
            <img src="/assets/mainPage/bcode/bcode-pic.png" alt="" class="plLayer bcodeLayer" />
          </div>
        </div>
      </div>

      <div class="topCaption">
        <h2 data-speed="2.5">
          <a href="portfolio/bcode/">Website & logo<br/>for clinic</a>
        </h2>
      </div>

      <div class="fullScreen text-center" data-speed="4">
        <div class="table">
          <div class="cell">
            <span class="bcode-logo">BCODE Beauty</span>
          </div>
        </div>
      </div>
      <div class="topCaption link-shadow">
        <div class="h1" data-speed="2.5">
          <a href="portfolio/bcode/">Website & logo<br/>for clinic</a>
        </div>
      </div>
    </div>


    <div id="digitalTrip">
      <div data-speed='0.75' class='text-center fullScreen'>
        <div class="table">
          <div class="cell">
            <img class="DT-img" src="/assets/mainPage/dt/stones-back.png" alt="" />
          </div>
        </div>
      </div>
      <div class="fullScreen text-center" data-speed="1">
        <div class="table">
          <div class="cell">
            <img class="DT-img DT-upperImage" src="/assets/mainPage/dt/cat.png" alt="" />
          </div>
        </div>
      </div>
      <div data-speed="2" class="fullScreen text-center">
        <div class="table">
          <div class="cell">
            <img class="DT-img DT-logo" src="/assets/mainPage/dt/dt-logo.png" alt="" />
          </div>
        </div>
      </div>
      <div data-speed='4' class='text-center fullScreen'>
        <div class="table">
          <div class="cell">
            <img class="DT-img DT-frontImg" src="/assets/mainPage/dt/stones-blurred.png" alt="" />
          </div>
        </div>
      </div>
      <div data-speed='2' class='text-center fullScreen'>
        <div class="table">
          <div class="cell">
            <img class="DT-img" src="/assets/mainPage/dt/stones-front.png" alt="" />
          </div>
        </div>
      </div>
      <div class="topCaption">
        <h2 data-speed="1.6"><a href="portfolio/trip/">WebGL game</a></h2>
      </div>
    </div>


    <div id="kynsi" class="">
      <div class="fullScreen text-center" data-speed="1">
        <img class="kynsi-pic" src="/assets/mainPage/kynsi/cryingCyan.png" style="position:static !important" alt="" />
      </div>
      <div class="topCaption">
        <h2 data-speed="1.8"><a href="portfolio/kynsi/">Branding & Website</a></h2>
      </div>
      <div class="kynsi-text" data-speed="4">
        <img class="kynsi-text-img" src="/assets/mainPage/kynsi/kynsiLogo.png" alt="" />
      </div>
    </div>


    <div id="kis2015">
      <div class="fullScreen" data-speed="1.6">
        <div class="table">
          <div class="cell"><img src="/assets/mainPage/kis2015/kis-layer4.png" class="plLayer kisLayer04" alt="" /></div>
        </div>
      </div>
      <div class="fullScreen" data-speed="2">
        <div class="table">
          <div class="cell"><img src="/assets/mainPage/kis2015/kis-layer3.png" class="plLayer kisLayer03" alt="" /></div>
        </div>
      </div>
      <div class="fullScreen topCaption" data-speed="2">
        <h2>
          <a href="portfolio/kis2015/">
            <img src="/assets/mainPage/kis2015/kis-logo.png" class="kis-logo" alt="" />
            Promotional<br/>Website
          </a>
        </h2>
      </div>
      <div class="fullScreen" data-speed="2.5">
        <div class="table">
          <div class="cell"><img src="/assets/mainPage/kis2015/kis-layer2.png" class="plLayer kisLayer02" alt="" /></div>
        </div>
      </div>
      <div class="fullScreen" data-speed="7">
        <div class="table">
          <div class="cell"><img src="/assets/mainPage/kis2015/kis-layer1_800.png" class="plLayer kisLayer01" alt="" /></div>
        </div>
      </div>
      <div class="fullScreen topCaption link-shadow" data-speed="2.5">
        <div class="h1">
          <a href="portfolio/kis2015/">
            <img src="/assets/mainPage/kis2015/kis-logo.png" class="kis-logo" alt="" />
            Promotional<br/>Website
          </a>
        </div>
      </div>
    </div>


    <div id="bilet2u">

      <img data-speed="0.75" src="/assets/mainPage/bilet2u/l-1.png" class="b2u-l-1" alt="" />

      <div data-speed="1" class='fullScreen text-center'>

        <img src="/assets/mainPage/bilet2u/man.png" class="b2u-man" alt="" />
      </div>

      <img data-speed="1.5" src="/assets/mainPage/bilet2u/l0.png" class="b2u-l0" alt="" />
      <img data-speed="2.5" src="/assets/mainPage/bilet2u/l1.png" class="b2u-l1" alt="" />

      <div data-speed='2.5' class='text-center fillParent'>
        <img src="/assets/mainPage/bilet2u/logo.png" class="bilet2uLogo" alt="" />
      </div>

      <div class="topCaption yStack">
        <h2 data-speed="2.5">Ticket Sales<br/>Website</h2>
      </div>

      <div data-speed="4" class='b2u-confetti left'>
        <img src="/assets/mainPage/bilet2u/big_r_speckle.png" class="b2u-r-specle" alt="" />
      </div>
      <div data-speed="10" class='b2u-confetti right'>
        <img src="/assets/mainPage/bilet2u/big_y_speckle.png" class="b2u-y-specle" alt="" />
      </div>

      <div class="topCaption link-shadow">
        <div data-speed="2.5" class="h1">
          <a href="portfolio/bilet2u/">Ticket Sales<br/>Website</a>
        </div>
      </div>
    </div>
  </div>


  <div class="nav-animation">
    <img src="/assets/mouse.gif" alt="scroll" />
  </div>
  <a class="right-arrow">
    <img src="/assets/arrow_right.png" alt="" />
  </a>
  <a class="left-arrow">
    <img src="/assets/arrow_left.png" alt="" />
  </a>



    <div id="footer" class="Footer">
    <ul class="Footer-menu">

      <li class="team vAligner">
        <a class="link-st vItem" href="/we/">Hot Dot</a>
      </li>
      <li>
        <a class="link-st" href="/portfolio/">Portfolio</a>
      </li>
      <li class="location last">
        <a class="link-st" href="/contact/">contact</a>
      </li>
    </ul>
  </div>

  <%= javascript_include_tag 'application' %>
  <%= javascript_pack_tag "application" %> <!-- from app/javascript/packs/application.js -->
